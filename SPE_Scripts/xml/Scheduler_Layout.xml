<?xml version="1.0" encoding="utf-8"?>
<Scheduler>
    <Websites>
        <Website ScriptInternalName="RootWeb" relativeUrl="/">
            <Lists>
                <List Name="Scheduler_Log" Template="Generic list">
                    <Description>Log messages generated by the Scheduler's PowerShell scripts</Description>
                    <Fields>
                        <Field Name="TimeStamp" InternalName="Title" DisplayName="TimeStamp" Type="Text" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The original title field, renamed to TimeStamp</Description>
                        </Field>
                        <Field Name="Process" InternalName="Process" DisplayName="Process" Type="Text" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The Process field</Description>
                        </Field>
                        <Field Name="Thread" InternalName="Thread" DisplayName="Thread" Type="Text" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The Thread field</Description>
                        </Field>
                        <Field Name="Area" InternalName="Area" DisplayName="Area" Type="Text" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The Area Field</Description>
                        </Field>
                        <Field Name="Category" InternalName="Category" DisplayName="Category" Type="Text" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The Category Field</Description>
                        </Field>
                        <Field Name="EventId" InternalName="EventId" DisplayName="EventId" Type="Text" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The EventId Field</Description>
                        </Field>
                        <Field Name="Level" InternalName="Level" DisplayName="Level" Type="Text" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The Level field</Description>
                        </Field>
                        <Field Name="Message" InternalName="Message" DisplayName="Message" Type="Note" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The Message field</Description>
                        </Field>
                        <Field Name="Correlation" InternalName="Correlation" DisplayName="Correlation" Type="Text" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The Correlation field</Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_Countries" Template="Generic list">
                    <Description>List of all countries in which trainings are provided</Description>
                    <Fields>
                        <Field Name="Title" InternalName="Title" DisplayName="Title" Type="Text" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>Name of the Country</Description>
                        </Field>
                        <Field Name="Shortcut" InternalName="Shortcut" DisplayName="Shortcut" Type="Text" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>Shortcut to Country</Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_Locations" Template="Generic list">
                    <Description>List of all locations in which trainings are provided</Description>
                    <Fields>
                        <Field Name="Title" InternalName="Title" DisplayName="Title" Type="Text" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>Name of the Location or City</Description>
                        </Field>
                        <Field Name="Country" InternalName="Country" DisplayName="Country" Type="Lookup" Mandatory="True" DefaultValue="">
                            <Options>
                                <Lookup AllowMultipleSelect="False">
                                    <SourceList Name="Scheduler_Countries" />
                                    <SourceField Name="Title" />
                                </Lookup>
                            </Options>
                            <Description>References the related Country</Description>
                        </Field>
                        <Field Name="PcAvailable" InternalName="PcAvailable" DisplayName="PcAvailable" Type="Boolean" Mandatory="False" DefaultValue="True">
                            <Options />
                            <Description>Are Trainings-PCs available?</Description>
                        </Field>
                        <Field Name="NumPcAvailable" InternalName="NumPcAvailable" DisplayName="NumPcAvailable" Type="Number" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>How many PCs are available?</Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_Buildings" Template="Generic list">
                    <Description>List of all buildings in which trainings are provided</Description>
                    <Fields>
                        <Field Name="Title" InternalName="Title" DisplayName="Title" Type="Text" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>The name or number of the building</Description>
                        </Field>
                        <Field Name="Location" InternalName="Location" DisplayName="Location" Type="Lookup" Mandatory="True" DefaultValue="">
                            <Options>
                                <Lookup AllowMultipleSelect="False">
                                    <SourceList Name="Scheduler_Locations" />
                                    <SourceField Name="Title" />
                                </Lookup>
                            </Options>
                            <Description>The location in which the building is located</Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_Rooms" Template="Generic list">
                    <Description>List of all rooms in which trainings are provided</Description>
                    <Fields>
                        <Field Name="Title" InternalName="Title" DisplayName="Title" Type="Text" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>Name or number of the room</Description>
                        </Field>
                        <Field Name="Building" InternalName="Building" DisplayName="Building" Type="Lookup" Mandatory="True" DefaultValue="">
                            <Options>
                                <Lookup AllowMultipleSelect="False">
                                    <SourceList Name="Scheduler_Buildings" />
                                    <SourceField Name="Title" />
                                </Lookup>
                            </Options>
                            <Description>The building in which the room is located</Description>
                        </Field>
                        <Field Name="MaxAllocation" InternalName="MaxAllocation" DisplayName="MaxAllocation" Type="Number" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>maximum number of seats all over</Description>
                        </Field>
                        <Field Name="NoTraineeSeats" InternalName="NoTraineeSeats" DisplayName="NoTraineeSeats" Type="Calculated" Mandatory="False" DefaultValue="">
                            <Options>
                                <Calculated Formula="=[MaxAllocation]-1" />
                            </Options>
                            <Description>Number of seats without the trainers seat</Description>
                        </Field>
                        <Field Name="NoPCs" InternalName="NoPCs" DisplayName="NoPCs" Type="Number" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>Number of available PCs</Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_Topics" Template="Generic list">
                    <Description>List of all topics to which trainings, modules and stages are provided</Description>
                    <Fields>
                        <Field Name="Title" InternalName="Title" DisplayName="Title" Type="Text" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>Name of the topic or technology</Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_Languages" Template="Generic list">
                    <Description>List of all languages in which trainings will be provided</Description>
                    <Fields>
                        <Field Name="Title" InternalName="Title" DisplayName="Title" Type="Text" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>Name of the language</Description>
                        </Field>
                        <Field Name="Shortcut" InternalName="Shortcut" DisplayName="Shortcut" Type="Text" Mandatory="Mandatory" DefaultValue="">
                            <Options />
                            <Description>Shortcut to the language</Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_KickOffItems" Template="Generic list">
                    <Description>List of KickOffItems which are combined by date and room</Description>
                    <Fields>
                        <Field Name="Title" InternalName="Title" DisplayName="Title" Type="Text" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>The identifier of the KickOff-Item</Description>
                        </Field>
                        <Field Name="Date" InternalName="Date" DisplayName="Date" Type="DateTime" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>The Date for the KickOff</Description>
                        </Field>
                        <Field Name="Location" InternalName="Location" DisplayName="Location" Type="Lookup" Mandatory="Mandatory" DefaultValue="">
                            <Options>
                                <Lookup AllowMultipleSelect="False">
                                    <SourceList Name="Scheduler_Locations" />
                                    <SourceField Name="Title" />
                                </Lookup>
                            </Options>
                            <Description>The location for the Kickoff</Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_Stages" Template="Generic list">
                    <Description>List of all stages provided</Description>
                    <Fields>
                        <Field Name="Title" InternalName="Title" DisplayName="Title" Type="Text" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>Name of the stage</Description>
                        </Field>
                        <Field Name="Topics" InternalName="Topics" DisplayName="Topics" Type="Lookup" Mandatory="True" DefaultValue="">
                            <Options>
                                <Lookup AllowMultipleSelect="True">
                                    <SourceList Name="Scheduler_Topics" />
                                    <SourceField Name="Title" />
                                </Lookup>
                            </Options>
                            <Description>The topics related to the stage</Description>
                        </Field>
                        <Field Name="Description" InternalName="Description" DisplayName="Description" Type="Note" Mandatory="False" DefaultValue="">
                            <Options>
                                <Note />
                            </Options>
                            <Description>The Description of the stage</Description>
                        </Field>
                        <Field Name="KickOff" InternalName="KickOff" DisplayName="KickOff" Type="Lookup" Mandatory="False" DefaultValue="">
                            <Options>
                                <Lookup AllowMultipleSelect="False">
                                    <SourceList Name="Scheduler_KickOffItems" />
                                    <SourceField Name="Title" />
                                </Lookup>
                            </Options>
                            <Description>Date and location for kickoff</Description>
                        </Field>
                        <Field Name="Published" InternalName="Published" DisplayName="Published" Type="Boolean" Mandatory="True" DefaultValue="False">
                            <Options />
                            <Description>Set to true to publish the stage</Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_Modules" Template="Generic list">
                    <Description>List of all modules provided</Description>
                    <Fields>
                        <Field Name="Title" InternalName="Title" DisplayName="Title" Type="Text" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>The name of the module</Description>
                        </Field>
                        <Field Name="Description" InternalName="Description" DisplayName="Description" Type="Note" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>The description of the module</Description>
                        </Field>
                        <Field Name="Stage" InternalName="Stage" DisplayName="Stage" Type="Lookup" Mandatory="True" DefaultValue="">
                            <Options>
                                <Lookup AllowMultipleSelect="False">
                                    <SourceList Name="Scheduler_Modules" />
                                    <SourceField Name="Title" />
                                </Lookup>
                            </Options>
                            <Description>The stage where the module is related to</Description>
                        </Field>
                        <Field Name="Topics" InternalName="Topics" DisplayName="Topics" Type="Lookup" Mandatory="False" DefaultValue="">
                            <Options>
                                <Lookup AllowMultipleSelect="True">
                                    <SourceList Name="Scheduler_Modules" />
                                    <SourceField Name="Title" />
                                </Lookup>
                            </Options>
                            <Description>The Topics related to the module</Description>
                        </Field>
                        <Field Name="Published" InternalName="Published" DisplayName="Published" Type="Boolean" Mandatory="False" DefaultValue="False">
                            <Options />
                            <Description>Set to True if module shall be published</Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_Trainings" Template="Generic list">
                    <Description>List of all trainings provided</Description>
                    <Fields>
                        <Field Name="Title" InternalName="Title" DisplayName="Title" Type="Text" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>The name of the training</Description>
                        </Field>
                        <Field Name="Description" InternalName="Description" DisplayName="Description" Type="Note" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>The description of the training</Description>
                        </Field>
                        <Field Name="Module" InternalName="Module" DisplayName="Module" Type="Lookup" Mandatory="True" DefaultValue="">
                            <Options>
                                <Lookup AllowMultipleSelect="False">
                                    <SourceList Name="Scheduler_Modules" />
                                    <SourceField Name="Title" />
                                </Lookup>
                            </Options>
                            <Description>The module to where the training is related to</Description>
                        </Field>
                        <Field Name="Language" InternalName="Language" DisplayName="Language" Type="Lookup" Mandatory="True" DefaultValue="">
                            <Options>
                                <Lookup AllowMultipleSelect="False">
                                    <SourceList Name="Scheduler_Trainings" />
                                    <SourceField Name="Shortcut" />
                                </Lookup>
                            </Options>
                            <Description>The language in which the training is provided</Description>
                        </Field>
                        <Field Name="Published" InternalName="Published" DisplayName="Published" Type="Boolean" Mandatory="False" DefaultValue="False">
                            <Options />
                            <Description>Set to True if module shall be published</Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_Dates" Template="Generic list">
                    <Description>List of all dates at which training session are offered</Description>
                    <Fields>
                        <Field Name="Title" InternalName="Title" DisplayName="Title" Type="Text" Mandatory="True" DefaultValue="">
                            <Options />
                            <Description>The name or identifier of the trainings date</Description>
                        </Field>
                        <Field Name="Description" InternalName="Description" DisplayName="Description" Type="Note" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The description of the date</Description>
                        </Field>
                        <Field Name="Training" InternalName="Training" DisplayName="Training" Type="Lookup" Mandatory="True" DefaultValue="">
                            <Options>
                                <Lookup AllowMultipleSelect="False">
                                    <SourceList Name="Scheduler_Trainings" />
                                    <SourceField Name="Title" />
                                </Lookup>
                            </Options>
                            <Description>The training to which the date is related to</Description>
                        </Field>
                        <Field Name="Trainer" InternalName="Trainer" DisplayName="Trainer" Type="Lookup" Mandatory="True" DefaultValue="">
                            <Options>
                                <Lookup AllowMultipleSelect="False">
                                    <SourceList Name="Scheduler_Trainer" />
                                    <SourceField Name="Title" />
                                </Lookup>
                            </Options>
                            <Description>The trainer leading the training's date</Description>
                        </Field>
                        <Field Name="Duration" InternalName="Duration" DisplayName="Duration" Type="Number" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The duration of the date in hours</Description>
                        </Field>
                        <Field Name="Start" InternalName="Start" DisplayName="Start" Type="DateTime" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The starting time for the trainings date</Description>
                        </Field>
                        <Field Name="End" InternalName="End" DisplayName="End" Type="DateTime" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The ending time for the trainings date</Description>
                        </Field>
                        <Field Name="TrainingsSeats" InternalName="TrainingsSeats" DisplayName="TrainingsSeats" Type="Number" Mandatory="False" DefaultValue="">
                            <Options>
                                <ReadOnly />
                            </Options>
                            <Description>The overall number of trainings seats. (Will be edited by script)</Description>
                        </Field>
                        <Field Name="FreeSeats" InternalName="FreeSeats" DisplayName="FreeSeats" Type="Number" Mandatory="False" DefaultValue="">
                            <Options>
                                <ReadOnly />
                            </Options>
                            <Description>The number of available trainings seats. (Will be edited by script)</Description>
                        </Field>
                        <Field Name="BookedSeats" InternalName="BookedSeats" DisplayName="BookedSeats" Type="Number" Mandatory="False" DefaultValue="">
                            <Options>
                                <ReadOnly />
                            </Options>
                            <Description>Number of booked seats.(Will be edited by script)</Description>
                        </Field>
                        <Field Name="TrainingsPCs" InternalName="TrainingsPCs" DisplayName="TrainingsPCs" Type="Number" Mandatory="False" DefaultValue="">
                            <Options>
                                <ReadOnly />
                            </Options>
                            <Description>The overall number of trainings PC's. (Will be edited by script)</Description>
                        </Field>
                        <Field Name="FreePCs" InternalName="FreePCs" DisplayName="FreePCs" Type="Number" Mandatory="False" DefaultValue="">
                            <Options>
                                <ReadOnly />
                            </Options>
                            <Description>The number of available PC's. (Will be edited by script)</Description>
                        </Field>
                        <Field Name="MinSubscriptions" InternalName="MinSubscriptions" DisplayName="MinSubscriptions" Type="Number" Mandatory="True" DefaultValue="0">
                            <Options />
                            <Description>Minimum Number of booked subscriptions</Description>
                        </Field>
                        <Field Name="Published" InternalName="Published" DisplayName="Published" Type="Boolean" Mandatory="False" DefaultValue="False">
                            <Options />
                            <Description>Set to True if module shall be published</Description>
                        </Field>
                        <Field Name="Bookable" InternalName="Bookable" DisplayName="Bookable" Type="Boolean" Mandatory="False" DefaultValue="False">
                            <Options />
                            <Description>Set to true if date is bookable, initially set by creator, later changed by script</Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_Trainers" Template="Generic list">
                    <Description>List of all trainers</Description>
                    <Fields>
                        <Field Name="" InternalName="" DisplayName="" Type="" Mandatory="False" DefaultValue="">
                            <Options>
                                <Option Type="">
                                    <Choices>
                                        <Choice Name="" />
                                    </Choices>
                                    <Lookup AllowMultipleSelect="False">
                                        <SourceList Name="" />
                                        <SourceField Name="" />
                                    </Lookup>
                                    <Calculated Formula="" />
                                    <Taxonomy>
                                        <TermGroup Id="" />
                                        <TermSet Id="" />
                                    </Taxonomy>
                                </Option>
                            </Options>
                            <Description></Description>
                        </Field>
                    </Fields>
                </List>
                <List Name="Scheduler_Subscriptions" Template="Generic list">
                    <Description>List of the subscriptions created primarily by endusers</Description>
                    <Fields>
                        <Field Name="SubscriptionID" InternalName="Title" DisplayName="SubscriptionId" Type="Text" Mandatory="False" DefaultValue="">
                            <Options>
                                <ReadOnly />
                            </Options>
                            <Description>The id of the subscription.(Will be calculated by script)</Description>
                        </Field>
                        <Field Name="Creator" InternalName="Author" DisplayName="Creator" Type="User" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>The creator of the subscription.(Will be automatically generated by SharePoint.)</Description>
                        </Field>
                        <Field Name="CreateForAnotherUser" InternalName="CreateForAnotherUser" DisplayName="CreateForAnotherUser" Type="Boolean" Mandatory="False" DefaultValue="False">
                            <Options />
                            <Description>Set to True, if subscription will be created for another user.</Description>
                        </Field>
                        <Field Name="Subscriber" InternalName="Subscriber" DisplayName="Subscriber" Type="User" Mandatory="False" DefaultValue="">
                            <Options />
                            <Description>If CreateForAnotherUser is set to False, Subscriber will automatically set to the value of Creator and field will be hidden in forms.</Description>
                        </Field>
                        <Field Name="State" InternalName="State" DisplayName="State" Type="Choice" Mandatory="False" DefaultValue="Registered">
                            <Options>
                                <Choices>
                                    <Choice Name="Registered" />
                                    <Choice Name="Booked" />
                                    <Choice Name="Completed" />
                                    <Choice Name="No Show" />
                                    <Choice Name="Canceled" />
                                </Choices>
                            </Options>
                            <Description>The state of the subscription.</Description>
                        </Field>
                        <Field Name="NeedPC" InternalName="NeedPC" DisplayName="NeedPC" Type="Boolean" Mandatory="False" DefaultValue="No">
                            <Options />
                            <Description>Set to True if Subscriber has no PC to bring to the Training.</Description>
                        </Field>
                    </Fields>
                </List>
            </Lists>
        </Website>
        <Website ScriptInternalName="web_de" relativeUrl="/de-de">
            <Lists>
                <List Name="" Template="Generic list" >
                    <Description></Description>
                    <Fields>
                        <Field Name="" InternalName="" DisplayName="">
                             Type="" Mandatory="False" DefaultValue="">
                            <Options>
                                <Option Type="">
                                    <Choices>
                                        <Choice Name="" />
                                    </Choices>
                                    <Lookup AllowMultipleSelect="False">
                                        <SourceList Name="" />
                                        <SourceField Name="" />
                                    </Lookup>
                                    <Calculated Formula="" />
                                    <Taxonomy>
                                        <TermGroup Id="" />
                                        <TermSet Id="" />
                                    </Taxonomy>
                                </Option>
                            </Options>
                            <DefaultValue Value="" />
                            <Description></Description>
                            <Mandatory Value="" />
                        </Field>
                    </Fields>
                </List>
            </Lists>
        </Website>
        <Website ScriptInternalName="web_en" relativeUrl="/en-us">
            <Lists>
                <List Name="" Template="Generic list" >
                    <Description></Description>
                    <Fields>
                        <Field Name="" InternalName="" DisplayName="">
                             Type="" Mandatory="False" DefaultValue="">
                            <Options>
                                <Option Type="">
                                    <Choices>
                                        <Choice Name="" />
                                    </Choices>
                                    <Lookup AllowMultipleSelect="False">
                                        <SourceList Name="" />
                                        <SourceField Name="" />
                                    </Lookup>
                                    <Calculated Formula="" />
                                    <Taxonomy>
                                        <TermGroup Id="" />
                                        <TermSet Id="" />
                                    </Taxonomy>
                                </Option>
                            </Options>
                            <DefaultValue Value="" />
                            <Description></Description>
                            <Mandatory Value="" />
                        </Field>
                    </Fields>
                </List>
            </Lists>
        </Website>
    </Websites>
</Scheduler>